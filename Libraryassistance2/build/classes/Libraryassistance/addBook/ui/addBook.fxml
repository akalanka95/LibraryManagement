<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="274.0" prefWidth="299.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Libraryassistance.addBook.ui.AddBookController">
    <children>
      <VBox prefHeight="219.0" prefWidth="283.0">
         <children>
            <JFXTextField fx:id="txtTitle" promptText="Book Title">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtId" promptText="Book Id">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtAuthor" promptText="Book Author">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtPublisher" promptText="Publisher">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
         </children>
      </VBox>
      <HBox layoutX="-2.0" layoutY="205.0" prefHeight="54.0" prefWidth="299.0">
         <children>
            <JFXButton fx:id="btnSave" onAction="#addBook" prefHeight="45.0" prefWidth="138.0" text="Save" />
            <JFXButton fx:id="btnCancel" onAction="#Cancel" prefHeight="46.0" prefWidth="143.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
